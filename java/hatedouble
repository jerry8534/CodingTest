class Solution {
    public int[] solution(int n) {
        int a;
        if(n%2 != 0){
            a = n/2+1;
        }else{
            a = n/2;
        }
        int[] answer = new int[a];
        for(int i=1; i<=n; i++){
            if(i%2 != 0) answer[i/2] = i;
        }
        return answer;
    }
}