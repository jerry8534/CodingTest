class Solution {
    public int solution(int hp) {
        int answer = 0;
        int r = hp;
        int a = 0;
        int b = 0;
        int c = 0;
        if(r>=5){
            a = r/5;
            r = r%5;
        }
        if(r >= 3){
            b = r / 3;
            r = r % 3;
        }
        if(r > 0){
            c = r;
        }
        answer = a+b+c;
        return answer;
    }
}