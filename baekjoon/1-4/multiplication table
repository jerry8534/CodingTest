import java.util.*;
import java.io.*;

public class Main{
    public static void main(String[] args) throws IOException{
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        String x = br.readLine();
        int N = Integer.parseInt(x);
        for(int i = 1; i <= 9; i++){
            System.out.println(N + " * " + i + " = " + (N*i));
        }
    }
}